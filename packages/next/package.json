{
  "name": "@no-auth/next",
  "version": "0.0.1",
  "description": "Auth library for nextjs",
  "homepage": "https://github.com/Sagar-v4/no-auth/blob/master/packages/next/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sagar-v4/no-auth.git"
  },
  "bugs": {
    "type": "git",
    "url": "git+https://github.com/Sagar-v4/no-auth/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "publish:npm": "npm publish --access public"
  },
  "keywords": [
    "no auth",
    "auth"
  ],
  "author": "Sagar Variya",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "19.0.8",
    "next": "^15.2.4",
    "react": "^19.0.0",
    "swr": "^2.2.5",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "next": "^15.0.0 || ^14.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "swr": "^2.0.0"
  }
}
